    "vue-notification": "^1.3.13",
    "vue-router": "^3.0.1",
    "vuetify": "^1.1.13",
    "vuex": "^3.0.1"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    },
    "testModule": "tests/main.js"
